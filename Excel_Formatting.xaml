                  </Assign.To>
                  <Assign.Value>
                    <InArgument x:TypeArguments="s:Type">[GetType(String)]</InArgument>
                  </Assign.Value>
                </Assign>
              </Sequence>
            </ActivityAction>
          </ui:ForEach.Body>
        </ui:ForEach>
